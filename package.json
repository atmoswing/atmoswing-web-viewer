    "i18next": "^25.4.2",
    "jspdf": "^3.0.2",
    "ol": "^10.6.1",
    "ol-layerswitcher": "^4.1.2",
    "proj4": "^2.11.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.7.3",
